<p>

<table id="tWorkLoadData" class="sticky-enabled table">
	<thead>
		<tr>
			<th>JOB</th>
			#foreach($file in $filelist)
			<th> $file </th>
			#end
		</tr>
	</thead>
	<tbody>
		#set($count=-1)
		#foreach($job in $workload.get($group))
  		#set($count = $count+1)
		<tr class="odd">
			<td> $job </td>
			#foreach($value in $tabledata.get($group).get($count))
			<td>$!value%</td>
		#end
		</tr>
		#end
	</tbody>
</table>
